{
  "compilerOptions": {
    /* Language and Environment */
    "target": "es2017",
    /* Modules */
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    /* Type Checking */
    "strictPropertyInitialization": false,
    "strict": false,
    "sourceMap": true,
    "outDir": "dist",
    "lib": ["esnext"],
    //"typeRoots": ["./src/@types", "node_modules/@types"],
  }
}
